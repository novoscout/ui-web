{
  "name": "ui-web",
  "version": "0.0.1",
  "description": "Template for websites using Novoscout services",
  "author": "austinjp",
  "main": "src/index.js",
  "license": "AGPL-3.0-or-later",
  "homepage": "https://github.com/novoscout/ui-web#readme",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/novoscout/ui-web.git"
  },
  "scripts": {
    "test": "       echo \"Error: no test specified\" && exit 1",
    "analytics": "  webpack --config webpack.analytics.js --mode production  --env production  --progress --color && yarn serve",
    "build": "      webpack --config webpack.prod.js      --mode production  --env production  --progress --color",
    "build-dev": "  webpack --config webpack.dev.js       --mode development --env development --progress --color",
    "build-debug": "webpack --config webpack.debug.js     --mode development --env development --progress --color",
    "compress": "   yarn gzip",
    "gzip": "       utils/compress-gzip",
    "brotli": "     utils/compress-brotli",
    "serve": "      node_modules/node-static/bin/cli.js -a 0.0.0.0 --spa -p 3000 ./dist",
    "serve-dev": "  node_modules/node-static/bin/cli.js -a 0.0.0.0 --spa -p 8080 ./dev"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@babel/plugin-transform-regenerator": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "copy-webpack-plugin": "^9.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "lorem-ipsum": "^2.0.4",
    "node-static": "^0.7.11",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "webpack": "^5.53.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.3.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "core-js": "^3.18.2",
    "cxs": "^6.2.0",
    "preact": "^10.5.14",
    "preact-responder-event-plugin": "^1.0.16",
    "preact-router": "^3.2.1"
  }
}
