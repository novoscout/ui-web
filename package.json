{
  "name": "ui-web",
  "version": "0.0.1",
  "description": "Template for websites using Novoscout services",
  "author": "austinjp",
  "main": "src/index.js",
  "license": "AGPL-3.0-or-later",
  "homepage": "https://github.com/novoscout/ui-web#readme",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/novoscout/ui-web.git"
  },
  "scripts": {
    "test": "       echo \"Error: no test specified\" && exit 1",
    "analytics": "  webpack --config webpack.analytics.js --mode production  --env production  --progress --color",
    "build": "      webpack --config webpack.prod.js      --mode production  --env production  --progress --color",
    "build-dev": "  webpack --config webpack.dev.js       --mode development --env development --progress --color",
    "build-debug": "webpack --config webpack.debug.js     --mode development --env development --progress --color",
    "compress": "   yarn gzip",
    "gzip": "       utils/compress-gzip",
    "brotli": "     utils/compress-brotli",
    "serve": "      webpack-dev-server --config ./webpack.prod.js --node-env production",
    "serve-dev": "  webpack-dev-server --config ./webpack.dev.js  --node-env development"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/plugin-syntax-decorators": "^7.16.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@babel/plugin-transform-regenerator": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "copy-webpack-plugin": "^9.0.1",
    "css-minimizer-webpack-plugin": "^3.1.3",
    "cssnano": "^5.0.10",
    "eslint": "^8.2.0",
    "eslint-config-preact": "^1.2.0",
    "eslint-plugin-preact": "^0.1.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "file-loader": "^6.2.0",
    "html-minimizer-webpack-plugin": "^3.3.1",
    "html-webpack-plugin": "^5.3.2",
    "lorem-ipsum": "^2.0.4",
    "mini-css-extract-plugin": "^2.4.4",
    "ngraph.graph": "^19.1.0",
    "ngraph.path": "^1.3.1",
    "node-static": "^0.7.11",
    "path": "^0.12.7",
    "postcss": "^8.3.11",
    "prop-types": "^15.7.2",
    "webpack": "^5.53.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.3.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "core-js": "^3.18.2",
    "cxs": "^6.2.0",
    "ngraph.fromjson": "^3.1.0",
    "ngraph.tojson": "^1.1.0",
    "preact": "^10.5.14",
    "preact-async-route": "^2.2.1",
    "preact-responder-event-plugin": "^1.0.16",
    "preact-router": "^3.2.1",
    "share-api-polyfill": "^1.0.21"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true,
      "node": false
    },
    "extends": [
      "preact"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 13,
      "sourceType": "module"
    },
    "plugins": [
      "react",
      "unused-imports"
    ],
    "rules": {
      "prefer-template": "off",
      "no-unused-vars": "off",
      "unused-imports/no-unused-imports": "error",
      "unused-imports/no-unused-vars": [
        "warn",
        {
          "vars": "all",
          "varsIgnorePattern": "^_",
          "args": "after-used",
          "argsIgnorePattern": "^_"
        }
      ]
    }
  }
}
