[
  {
    'article': {
      'front': {
        'article-meta': {
          'title-group': {
            'article-title': 'Offspring of Parents with Chronic Pain: A Systematic Review and Meta-Analysis of Pain, Health, Psychological, and Family Outcomes'
          },
          'contrib-group': {
            'contrib': [
              {
                '@contrib-type': 'author',
                'string-name': 'Kristen Sarah Higgins'
              }, {
                '@contrib-type': 'author',
