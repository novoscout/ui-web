              }, {
                '@contrib-type': 'author',
                'string-name': 'Christine T Chambers'
              }, {
                '@contrib-type': 'author',
                'string-name': 'Anna C Wilson'
              }, {
                '@contrib-type': 'author',
                'string-name': 'Line Caes'
              }, {
                '@contrib-type': 'author',
                'string-name': 'Alexander J Clark'
              }, {
                '@contrib-type': 'author',
